{
  "name": "messenger",
  "version": "1.0.0",
  "description": "Учебный проект Yandex Praktikum Middle Frontend",
  "type": "commonjs",
  "scripts": {
    "dev": "parcel src/index.html --no-source-maps",
    "build": "parcel build ./src/views/*.hbs --no-source-maps --no-cache --public-url ./",
    "server": "node app.js"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "pages": "./src/pages",
    "controllers": "./src/controllers",
    "utils": "./src/utils"
  },
  "author": "Aleksey Kripakov",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "parcel": "2.7.0",
    "stylelint": "14.16.1",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "eslint-plugin-react": "7.31.11",
    "handlebars": "4.7.7",
    "nanoid": "3.3.1",
    "sass": "1.57.1"
  }
}
